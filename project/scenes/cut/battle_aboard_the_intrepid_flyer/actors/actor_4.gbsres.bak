            "id": "6aff27d4-a783-4c8c-8020-1f2c1fe86598"
          },
          {
            "command": "EVENT_TEXT",
            "args": {
              "text": [
                "Jem: could I \ndisable those.I \ndislike most AI"
              ],
              "avatarId": "5d1a25ac-6d4f-40b5-9065-c9e137ae5c0e"
            },
            "id": "5e8234a0-eb22-4a50-a926-531a2de86886"
